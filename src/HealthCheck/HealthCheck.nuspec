<?xml version="1.0" encoding="utf-8"?>
<package>
	<metadata>
		<id>MrRabbit.HealthChecks.Container.Client</id>
		<version>8.0.0</version>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<projectUrl>https://github.com/lkroliko/HealthCheck</projectUrl>
		<title>HealthCheck</title>
		<owners>MrRabbit</owners>
		<authors>MrRabbit</authors>
		<tags>
			HealthCheck HealthChecks Health Container Docker Podman
		</tags>
		<description>Health checker client for .net linux container images. Replace 'curl --silent --fail http://localhost/hc || exit 1' with '["CMD", "dotnet", "/app/healthcheck.dll", "http://localhost:8080/hc" ]'.</description>
		<license type="expression">MIT</license>
		<icon>icon.png</icon>
		<readme>docs\readme.md</readme>
		<contentFiles>
			<files include="**\*" buildAction="None" copyToOutput="true" flatten="true" />
		</contentFiles>
	</metadata>
	<dependencies>
		<group targetFramework="net8.0"/>
	</dependencies>
	<files>
		<file src="..\..\README.md" target="docs\readme.md" />
		<file src="Resources\icon.png" target="" />
		<file src="HealthCheck.props" target="build\MrRabbit.HealthChecks.Container.Client.props" />

		<file src="bin\Release\net8.0\linux-x64\healthcheck.dll" target="contentFiles\any\any\HealthCheck"/>
		<file src="bin\Release\net8.0\linux-x64\healthcheck.runtimeconfig.json" target="contentFiles\any\any\HealthCheck" />
	</files>
</package>
